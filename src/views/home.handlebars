<h1 class="titulo">Productos</h1>
<div class="container">
    {{#each productos}}
    <div class="product">
        <h4>ID: {{this.id}}</h4>
        <h3>{{this.title}}</h3>
        <p>code: {{this.code}}</p>
        <p>$ {{this.price}}</p>
        <p>Stock: {{this.stock}}</p>
        
    </div>

    {{/each}}
</div>